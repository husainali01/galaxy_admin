<?xml version="1.0" encoding="utf-8"?><LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="match_parent"    android:layout_height="match_parent"    android:orientation="vertical"    android:paddingTop="15dp"    android:paddingBottom="15dp"    android:paddingStart="15dp"    tools:context=".MainActivity">    <LinearLayout        android:layout_width="wrap_content"        android:layout_height="wrap_content">        <CheckBox            android:id="@+id/checkBox1"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            style="?android:attr/starStyle"            android:backgroundTint="#ece447"            android:layout_gravity="center_vertical"            />        <TextView            android:id="@+id/textView1"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:text="User Name"            android:textSize="19sp"            android:layout_gravity="center_vertical"            android:textAppearance="?android:attr/textAppearanceLarge"            />    </LinearLayout>    <FrameLayout        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:orientation="horizontal">        <TextView            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:textAppearance="?android:attr/textAppearanceMedium"            android:text="Medium Text"            android:textSize="16sp"            android:drawableLeft="@drawable/ic_person_black_24dp"            android:paddingTop="5dp"            android:id="@+id/textView"            />        <TextView            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:textAppearance="?android:attr/textAppearanceMedium"            android:text="Operational"            android:layout_marginRight="20dp"            android:textSize="14sp"            android:textStyle="italic"            android:textColor="#58a8ea"            android:layout_gravity="right"            android:id="@+id/textView4"            />    </FrameLayout>    <FrameLayout        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:orientation="horizontal">        <TextView            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:textAppearance="?android:attr/textAppearanceSmall"            android:text="Small Text"            android:textColor="#000000"            android:gravity="start|center"            android:layout_gravity="center_vertical"            android:id="@+id/textView3"            />        <LinearLayout            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_gravity="right">            <ImageView                android:layout_width="24dp"                android:layout_height="24dp"                android:layout_marginRight="3dp"                android:layout_marginBottom="5dp"                android:src="@drawable/ic_closed_32dp"/>            <TextView                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:textAppearance="?android:attr/textAppearanceSmall"                android:text="31/03/2016"                android:layout_gravity="right|center"                android:layout_marginRight="20dp"                android:id="@+id/textView5"                />        </LinearLayout>    </FrameLayout>    <FrameLayout        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:orientation="horizontal">        <LinearLayout            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:textAppearance="?android:attr/textAppearanceSmall"            android:orientation="horizontal"            android:layout_gravity="left|center">        <TextView            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:textAppearance="?android:attr/textAppearanceSmall"            android:text="Raised"            android:layout_gravity="center_vertical"            android:id="@+id/raised"            />        <TextView            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:textAppearance="?android:attr/textAppearanceSmall"            android:text="50%"            android:textColor="#000000"            android:layout_marginLeft="3dp"            android:layout_gravity="center_vertical"            android:id="@+id/raised_percent"            />        </LinearLayout>        <LinearLayout            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_gravity="right|center">        <TextView            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:textAppearance="?android:attr/textAppearanceMedium"            android:text="IAN Round"            android:layout_marginRight="3dp"            android:textSize="15sp"            android:layout_gravity="right"            android:id="@+id/ian_round"            />        <TextView            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:textAppearance="?android:attr/textAppearanceMedium"            android:text="₹ 25 L"            android:textColor="#000000"            android:layout_marginRight="20dp"            android:textSize="15sp"            android:layout_gravity="right"            android:id="@+id/ian_round_amount"            />        </LinearLayout>    </FrameLayout>    <FrameLayout        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:orientation="horizontal">        <LinearLayout            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:orientation="horizontal"            android:layout_gravity="left|center">            <TextView                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:textAppearance="?android:attr/textAppearanceSmall"                android:text="Remaining Investor"                android:layout_gravity="center_vertical"                android:id="@+id/remaining"                />            <TextView                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:textAppearance="?android:attr/textAppearanceSmall"                android:text="50"                android:textColor="#000000"                android:layout_marginLeft="3dp"                android:layout_gravity="center_vertical"                android:id="@+id/remaining_count"                />        </LinearLayout>        <LinearLayout            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_gravity="right|center">            <TextView                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:textAppearance="?android:attr/textAppearanceMedium"                android:text="My Commitment"                android:layout_marginRight="3dp"                android:textSize="15sp"                android:layout_gravity="right"                android:id="@+id/my_commitment"                />            <TextView                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:textAppearance="?android:attr/textAppearanceMedium"                android:text="₹ 25 L"                android:textColor="#000000"                android:layout_marginRight="20dp"                android:textSize="15sp"                android:layout_gravity="right"                android:id="@+id/my_commitment_amount"                />        </LinearLayout>    </FrameLayout>    <FrameLayout        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:layout_marginTop="5dp"        android:orientation="horizontal">        <TextView            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:textAppearance="?android:attr/textAppearanceSmall"            android:text="Sponsored"            android:textSize="12sp"            android:layout_marginRight="20dp"            android:paddingBottom="1dp"            android:paddingTop="1dp"            android:paddingEnd="4dp"            android:paddingStart="4dp"            android:textColor="#FFFFFF"            android:background="#3f8339"            android:id="@+id/textView6"            android:layout_gravity="right|top" />        <LinearLayout            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_gravity="left|center"            android:orientation="horizontal">            <ImageView                android:layout_width="28dp"                android:layout_height="28dp"                android:scaleType="fitXY"                android:id="@+id/imageView"                android:src="@drawable/imgpsh_fullsize"                android:layout_gravity="left|center" />            <TextView                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:textAppearance="?android:attr/textAppearanceMedium"                android:text="2"                android:layout_marginLeft="10dp"                android:layout_gravity="center"                android:textColor="#000000"                android:id="@+id/comments_counts"                />            <ImageView                android:layout_width="28dp"                android:layout_height="28dp"                android:scaleType="fitXY"                android:layout_marginLeft="30dp"                android:id="@+id/imageView1"                android:src="@drawable/ic_meeting_fullsize"                android:layout_gravity="left|center" />            <TextView                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:textAppearance="?android:attr/textAppearanceMedium"                android:text="2"                android:layout_marginLeft="10dp"                android:layout_gravity="center"                android:textColor="#000000"                android:id="@+id/meetings_counts"                />        </LinearLayout>    </FrameLayout></LinearLayout>